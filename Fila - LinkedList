import java.util.LinkedList;
//FIFO - Primeiro a entrar, primeiro a ser removido
public class Fila {
	public static void main(String[] args) {
		LinkedList<String> fila = new LinkedList<String>();
		fila.add("Andressa");
		fila.add("Miguel");
		fila.add("Jo√£o");
		fila.add("Lila");
		int tam = fila.size();
		
		for (int i = 0; i < tam; i++) {
			System.out.println(fila.getFirst());
			fila.removeFirst();
		}
	}
}
