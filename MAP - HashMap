
import java.util.HashMap;
import java.util.Iterator;

public class ImplHashMap {

	public static void main(String[] args) {
		HashMap<Integer, String> lhM = new HashMap<Integer, String>();
		lhM.put(1, "Carlos");
		lhM.put(2, "Andressa");
		lhM.put(3, "Marcos");
		lhM.put(4, "Carlos");
		Iterator<String> it = lhM.values().iterator();
		while (it.hasNext()) {
			String nome = it.next();
			System.out.println(nome);
		}
	}
}
