import java.util.LinkedList;

public class Pilha {
 //LIFO - Último a entrar, primeiro a sair
	public static void main(String[] args) {
		LinkedList<String> pilha = new LinkedList<String>();
		pilha.push("Miguel");
		pilha.push("Lila");
		pilha.push("João");
		pilha.push("Andressa");
		int tam = pilha.size();
		for (int i = 0; i < tam; i++) {
			System.out.println(pilha.pop());
		}
	}
}
